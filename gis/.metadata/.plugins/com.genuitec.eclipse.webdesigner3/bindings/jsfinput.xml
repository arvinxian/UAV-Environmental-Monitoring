<?xml version="1.0"?>
<bindings xmlns="http://www.mozilla.org/xbl" 
		  xmlns:xbl="http://www.mozilla.org/xbl" 
		  xmlns:html="http://www.w3.org/1999/xhtml">
	
	<binding id="inputText" extends="inputs.xml#input">
		<content>
			<html:input anonid="input" type="text" style="background-position: bottom right; background-repeat: no-repeat"/>
			<html:span style="display:none"><children/></html:span>
		</content>
		<implementation>
			<field name="inputImage">"data:image/gif;base64,"</field>
			<field name="styleNodeId">"input"</field>
		</implementation>
	</binding>
	
	<binding id="inputSecret" extends="inputs.xml#input">
		<content>
			<html:input anonid="input" type="password" style="background-position: bottom right; background-repeat: no-repeat"/>
			<html:span style="display:none"><children/></html:span>
		</content>
		<implementation>
			<field name="inputImage">"data:image/gif;base64,"</field>
			<field name="styleNodeId">"input"</field>
		</implementation>
	</binding>
	
</bindings>

<?xml version="1.0"?>
<bindings xmlns="http://www.mozilla.org/xbl" 
		  xmlns:html="http://www.w3.org/1999/xhtml">
	
	<binding id="inline" extends="inlinelabel.xml#label">
		<implementation>
			<property name="labelText">
				<getter><![CDATA[
					return "<"+ this.getAttribute("wd3_tagname") +">";
				]]></getter>
			</property>
		</implementation>
	</binding>
	
	<binding id="block" extends="blocklabel.xml#label">
		<implementation>
			<property name="labelText">
				<getter><![CDATA[
					return "<"+ this.getAttribute("wd3_tagname") +">";
				]]></getter>
			</property>
		</implementation>
	</binding>
	
</bindings>

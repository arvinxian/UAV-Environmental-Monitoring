<?xml version="1.0"?>
<bindings xmlns="http://www.mozilla.org/xbl" 
		  xmlns:xbl="http://www.mozilla.org/xbl" 
		  xmlns:html="http://www.w3.org/1999/xhtml">
	
	<binding id="checkbox" extends="inputs.xml#input">
		<content>
			<html:wd3span anonid="outer" class="custominput">
				<html:input anonid="input" type="checkbox" 
					style="background-position: bottom right; background-repeat: no-repeat"/><html:wd3span anonid="label">
				</html:wd3span></html:wd3span><html:span style="display:none"><children/></html:span>
		</content>
		<implementation>
			<field name="inputImage">"data:image/gif;base64,"</field>
			<field name="styleNodeId">"outer"</field>
			<field name="paddingRight">"7px"</field>
			<property name="inputLabel">
				<getter><![CDATA[
					var value = this.getAttribute('value');
					var retval = "";
	    			if (value == null || value == "") {
	    				retval = this.getBoundTextContent(-1);
					} else {
						retval = value;
					}
					return retval;
				]]></getter>
			</property>	
		</implementation>
	</binding>
	
